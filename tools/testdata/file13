pSrcDstUPlane, Qpl8u* pSrcDstVPlane, Qpl32s srcDstUVStep, const
QplIntraChromaPredMode_AVS predMode, const Qpl32s* pSrcNumCoeffs, Qpl32u cbp8x8, Qpl32u
chromaQP, Qpl32u edgeType    );

